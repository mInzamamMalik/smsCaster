    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.4.2",
    "gulp-uglify": "^1.4.1",
    "jison": "^0.4.15",
    "mocha": "^2.3.0",
    "mversion": "^1.10.0",
    "npm-check": "^4.0.1",
    "uglifyjs": "^2.4.10"
  },
  "engines": [
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/agershun/alasql.git"
  },
